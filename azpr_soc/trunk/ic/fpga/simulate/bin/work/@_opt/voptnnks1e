library verilog;
use verilog.vl_types.all;
entity uart_rx is
    port(
        clk             : in     vl_logic;
        reset           : in     vl_logic;
        rx_busy         : out    vl_logic;
        rx_end          : out    vl_logic;
        rx_data         : out    vl_logic_vector(7 downto 0);
        rx              : in     vl_logic
    );
end uart_rx;
